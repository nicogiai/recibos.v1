<h1>Lista de impuestos del periodo</h1>

<table>
  <thead>
    <tr>
      <th>Impuesto</th>
      <th>Importe</th>
      <th>Default vencimiento0</th>
      <th>Default vencimiento1</th>
      <th>Default vencimiento2</th>
      <th>Default vencimiento3</th>
      <th>Default vencimiento4</th>
      <th>Default vencimiento5</th>
      <th>Default vencimiento6</th>
      <th>Default vencimiento7</th>
      <th>Default vencimiento8</th>
      <th>Default vencimiento9</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @impuestos_por_periodo.each do |impuesto_por_periodo| %>
      <tr>
        <td><%= impuesto_por_periodo.impuesto %></td>
        <td><%= impuesto_por_periodo.importe %></td>
        <td><%= impuesto_por_periodo.default_vencimiento0 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento1 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento2 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento3 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento4 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento5 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento6 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento7 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento8 %></td>
        <td><%= impuesto_por_periodo.default_vencimiento9 %></td>
        <td><%= link_to 'Show', impuesto_por_periodo %></td>
        <td><%= link_to 'Edit', edit_impuestos_por_periodo_path(impuesto_por_periodo) %></td>
        <td><%= link_to 'Destroy', impuesto_por_periodo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo impuesto al periodo', new_impuestos_por_periodo_path %>
